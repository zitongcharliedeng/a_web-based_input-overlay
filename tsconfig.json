{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "allowJs": true,
    "checkJs": false,
    "noEmit": false,
    "outDir": "./compiled",
    "strict": false,
    "noImplicitAny": false,
    "skipLibCheck": true
  },
  "include": ["**/*.ts"],
  "exclude": ["compiled", "node_modules"]
}
