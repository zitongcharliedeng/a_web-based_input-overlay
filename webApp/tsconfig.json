{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "lib": ["ES2020", "DOM"],
    "allowJs": true,
    "checkJs": false,
    "noEmit": false,
    "outDir": "./browserInputOverlayView/_compiled",
    "rootDir": ".",
    "strict": false,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "esModuleInterop": true
  },
  "include": ["browserInputOverlayView/**/*.ts", "browserInputListeners/**/*.ts"],
  "exclude": ["node_modules", "browserInputOverlayView/_compiled"]
}
